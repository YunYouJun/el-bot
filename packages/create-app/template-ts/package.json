    "url": "https://www.yunyoujun.cn"
  },
  "license": "AGPL-3.0",
  "dependencies": {
    "el-bot": "^0.8.0-alpha.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "typescript": "^4.1.3"
  }
}
